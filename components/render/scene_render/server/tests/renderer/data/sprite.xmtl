<?xml version="1.0" encoding="utf-8"?>
<material_library>
  <layouts/>
  <texmaps/>
  <materials>
    <material id="sprite_material" program="flat" tags="transparent">
      <property name="Reflectivity" type="float" value="0.4"/>
      <property name="Transparency" type="float" value="1"/>
      <property name="Shininess" type="float" value="20"/>
      <property name="DiffuseColor" type="vector">
        1 1 1 1
      </property>
      <property name="AmbientColor" type="vector">
        0 0 0 1
      </property>
      <property name="SpecularColor" type="vector">
        0 0 0 0
      </property>
      <property name="EmissionColor" type="vector">
        0 0 0 1
      </property>
      <texmap semantic="diffuse" image="data/bottom.jpg" sampler="default"/>
    </material>
  </materials>
</material_library>
